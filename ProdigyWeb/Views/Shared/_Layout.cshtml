        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/perfil.js" asp-append-version="true"></script>
        <script src="~/js/payment.js" asp-append-version="true"></script>
        <script src="~/js/doughnut.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    }
    else if(ViewBag.Layout == "Dashboard")
    {
        <header>
            <nav class="nav">
            <!-- NAVBAR INÍCIO -->
            <div class="barra">
                <div class="topo">
                    <img src="~/img/svg/PRODIGY.svg" alt="" class="logo">
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Index"><img src="~/img/svg/Dashboard.svg" alt=""><p class="ps-3 m-0">Dashboard</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Caixa"><img src="~/img/svg/Balance.svg" alt=""><p class="ps-3 m-0">Fluxo de caixa</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Pedidos"><img src="~/img/svg/Tips.svg" alt=""><p class="ps-3 m-0">Pedidos</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Estoque"><img src="~/img/svg/Inventory.svg" alt=""><p class="ps-3 m-0">Estoque</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Funcionarios"><img src="~/img/svg/Employeers.svg" alt=""><p class="ps-3 m-0">Funcionários</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Agenda"><img src="~/img/svg/Calendar.svg" alt=""><p class="ps-3 m-0">Agenda</p></a>
                </div>
                <hr id="hr">
                <div class="baixo">
                    <a class="ps-3" asp-area="" asp-controller="Usuario" asp-action="Index"><img src="~/img/svg/Profile.svg" alt=""><p class="ps-3 m-0">Perfil</p></a>
                    <a class="ps-3" asp-area="" asp-controller="SHome" asp-action="Config"><img src="~/img/svg/Config.svg" alt=""><p class="ps-3 m-0">Configurações</p></a>
                        <a class="ps-3" asp-area="" asp-controller="Usuario" asp-action="Logout"><img src="~/img/svg/Subtract.svg" alt=""><p class="ps-3 m-0">Sair</p></a>
                </div>
            </div>
            <!-- NAVBAR FIM -->
            </nav>
            
        </header>


        <div class="container-fluid">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/perfil.js" asp-append-version="true"></script>
        <script src="~/js/payment.js" asp-append-version="true"></script>
        <script src="~/js/doughnut.js" asp-append-version="true"></script>
        <script src="~/js/estoque.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    }

